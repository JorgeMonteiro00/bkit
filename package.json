{
  "name": "bkit-gui",
  "version": "1.3.13",
  "description": "A bKit Client App",
  "productName": "bKit",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Isidro Vila Verde <jvverde@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "build": "quasar build -m electron",
    "build-linux": "quasar build -m electron linux -b builder",
    "build-win32": "quasar build -m electron win32 -b builder",
    "build-mac": "quasar build -m electron mac -t mat -b builder",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic"
  },
  "dependencies": {
    "@growthbunker/vuedarkmode": "^0.5.56",
    "@quasar/extras": "^1.8.2",
    "chokidar": "^3.3.1",
    "electron-log": "^4.0.6",
    "electron-store": "^5.1.0",
    "electron-updater": "^4.2.2",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "quasar": "^1.12.4",
    "upath": "^1.2.0",
    "vue-color": "^2.7.1",
    "vue-i18n": "^8.0.0"
  },
  "optionalDependencies": {
    "windows-scheduler": "^1.0.1",
    "elevator": "^2.2.3"
  },
  "devDependencies": {
    "@quasar/app": "^1.9.6",
    "@quasar/quasar-app-extension-icon-genie": "^1.1.3",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.1.0-beta.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "devtron": "^1.4.0",
    "electron": "^7.1.9",
    "electron-builder": "^22.6.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.2.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "majestic": "^1.2.24",
    "sinon": "^9.0.2",
    "wallaby-vue-compiler": "^1.0.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
